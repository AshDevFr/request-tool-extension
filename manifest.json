{
  "manifest_version": 2,

  "name": "Request tools Plugin",
  "description": "This extension allows you to scramble your requests",
  "version": "1.0",

  "devtools_page": "devtools.html",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ]
}
